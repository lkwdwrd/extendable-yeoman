  "files": [
    "lib"
  ],
  "keywords": [
    "yeoman",
    "generator",
    "plugin",
    "extension",
    "dynamic",
    "sub-generator",
    "base"
  ],
  "dependencies": {
    "globby": "^6.0.0",
    "lodash.compact": "^3.0.1",
    "yeoman-generator": "^0.24.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "chai": "^3.5.0",
    "eslint": "^3.3.1",
    "eslint-plugin-import": "^1.14.0",
    "mocha": "^3.0.2",
    "mocha-istanbul": "^0.3.0",
    "yeoman-test": "^1.4.0"
  }
}
