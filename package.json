{
  "name": "extendable-yeoman",
  "version": "0.2.1",
  "description": "Create Yeoman generators that support extensions and dynamic sub-generators automatically.",
  "license": "MIT",
  "main": "lib/main.js",
  "repository": "https://github.com/lkwdwrd/extendable-yeoman.git",
  "author": {
    "name": "Luke Woodward",
    "email": "woodward.lucas@gmail.com",
    "url": "https://lkwdwrd.com"
  },
  "contributors": [
    {
      "name": "Luke Woodward",
      "email": "woodward.lucas@gmail.com",
      "url": "https://lkwdwrd.com"
    }
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run compile && npm test",
    "pretest": "npm run lint && npm run compile",
    "test": "mocha --reporter spec 'test/**/*.js'",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -R spec test/**/*.js",
    "lint": "eslint src/**/*.js",
    "compile": "babel --presets es2015 -d lib/ src/"
